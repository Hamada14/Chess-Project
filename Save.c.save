#include <stdio.h>
#include "board.h"
void saveGame(void)
{
    FILE* save;
    save = fopen("save.bin","w");
    if(save == NULL)
    {
        return;
    }
    fputs(board,save);
    fclose(save);
}
void loadGame(void)
{
    FILE* load;
    load = fopen("save.bin","r");
    if(load == NULL)
    {
        return;
    }
    fread(board;)
}